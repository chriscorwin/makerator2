[ 	/*		"Define"				I am similar to a "Var_Set", but have one very important		difference: I check to see if the variable I am trying to		set has already been defined, and I will not over-write it		if it has.				If, on the other hand, it has not been defined yet, I will		then set it to the "Default".				Example of use: 				Define: -Name='myVar', -Default='myValue';	*/	define_tag('define', 		-required='name', 		-required='default');		if(!variable_defined(#name));			var(#name=#default);		/if;	/define_tag;]