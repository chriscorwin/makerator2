[	if(!lasso_tagExists('get_ListeratorSingular'));		define_tag(			'get_ListeratorSingular', 			-required='table', 			-priority='replace'			);				local('a_sql' = 'SELECT Singular FROM ' + $tablePrefix + 'listerators WHERE Table_Name = "' + #table + '"');				inline(					$authForDatabase, 					-table=($tablePrefix + #table), 					-sql=#a_sql					);						if(found_count == 1);								rows;									return(column('Singular'));								/rows;						else;								log_critical('ERROR: ' response_path ': ' #a_sql ', ' error_msg, -error);						/if;				/inline;		/define_tag;	/if;]