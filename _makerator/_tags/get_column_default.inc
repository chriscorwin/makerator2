[	/**	 * returns an array	 * requires 'table' and 'column' 	 * Returns an array of the possible values for a SET		use like:				get_set_options('Table_Name', 'Column_Name');	 */	define_tag(		'get_column_default', 		-required='table', 		-required='column', 		-priority='replace'		);			local('sql' = 'SHOW COLUMNS FROM '  + #table +  ' LIKE "' + #column + '"');			inline(				$authForDatabase, 				-table=#table, 				-sql=#sql			);					rows;						return(column('Default'));					/rows;			/inline;									error_code != 0 ? log_critical('Error from get_column_default: 'error_msg);	/define_tag;]