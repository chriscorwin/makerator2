[	if(!lasso_tagExists('get_ListeratorTitleFieldName'));		define_tag(			'get_ListeratorTitleFieldName', 			-required='table', 			-priority='replace'			);				local('a_sql' = 'SELECT title_field_name FROM ' + $tablePrefix + 'listerators WHERE Table_Name = "'#table'"');				inline(					$authForDatabase, 					-table=$tablePrefix + #table, 					-sql=#a_sql					);						if(found_count == 1);								rows;									return(column('title_field_name'));								/rows;						else;								log_critical('ERROR: ' response_path ': ' #a_sql ', ' error_msg, -error);						/if;				/inline;		/define_tag;	/if;]