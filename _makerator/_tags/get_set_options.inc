[	if(!lasso_tagExists('get_set_options'));		define_tag(			'get_set_options', 			-required='table', 			-required='column', 			-priority='replace'			);				local('sql' = 'SHOW COLUMNS FROM ' #table ' LIKE "'#column'"');				inline(					$authForDatabase, 					-table=#table, 					-sql=#sql					);						rows;							return(column('Type')->								substring(										5, 										(column('Type')->length - 5)									)->split(',')								);						/rows;				/inline;		/define_tag;	/if;]